{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies needed for static site'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

